{
  "expo": {
    "name": "StapuBox Calendar",
    "slug": "stapubox-calendar",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/stapubox_logo.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "plugins": [
      [
        "expo-build-properties",
        {
          "android": {
            "compileSdkVersion": 35,
            "targetSdkVersion": 35,
            "minSdkVersion": 24
          }
        }
      ]
    ],
    "scheme": "stapubox",
    "splash": {
      "image": "./assets/stapubox_logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/stapubox_logo.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.itspsd.stapuboxcalendar"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "9ef017d2-8811-4551-8e45-52790fd5ce17"
      }
    }
  }
}
